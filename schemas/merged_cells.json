{
  "$schema": "https://json-schema.org/draft/2020-12/schema",
  "description": "Compressed merged cell ranges using schema + items.",
  "properties": {
    "items": {
      "description": "Merged cell items as (r1, c1, r2, c2, v) tuples where rows are 1-based and columns are 0-based.",
      "items": {
        "maxItems": 5,
        "minItems": 5,
        "prefixItems": [
          {
            "type": "integer"
          },
          {
            "type": "integer"
          },
          {
            "type": "integer"
          },
          {
            "type": "integer"
          },
          {
            "type": "string"
          }
        ],
        "type": "array"
      },
      "title": "Items",
      "type": "array"
    },
    "schema": {
      "description": "Ordered field names for each item.",
      "items": {
        "enum": [
          "r1",
          "c1",
          "r2",
          "c2",
          "v"
        ],
        "type": "string"
      },
      "title": "Schema",
      "type": "array"
    }
  },
  "title": "MergedCells",
  "type": "object"
}