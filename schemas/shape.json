{
  "$schema": "https://json-schema.org/draft/2020-12/schema",
  "description": "Normal shape metadata.",
  "properties": {
    "h": {
      "anyOf": [
        {
          "type": "integer"
        },
        {
          "type": "null"
        }
      ],
      "default": null,
      "description": "Shape height (None if unknown).",
      "title": "H"
    },
    "id": {
      "anyOf": [
        {
          "type": "integer"
        },
        {
          "type": "null"
        }
      ],
      "default": null,
      "description": "Sequential shape id within the sheet (if applicable).",
      "title": "Id"
    },
    "kind": {
      "const": "shape",
      "default": "shape",
      "description": "Shape kind.",
      "title": "Kind",
      "type": "string"
    },
    "l": {
      "description": "Left offset (Excel units).",
      "title": "L",
      "type": "integer"
    },
    "rotation": {
      "anyOf": [
        {
          "type": "number"
        },
        {
          "type": "null"
        }
      ],
      "default": null,
      "description": "Rotation angle in degrees.",
      "title": "Rotation"
    },
    "t": {
      "description": "Top offset (Excel units).",
      "title": "T",
      "type": "integer"
    },
    "text": {
      "description": "Visible text content of the shape.",
      "title": "Text",
      "type": "string"
    },
    "type": {
      "anyOf": [
        {
          "type": "string"
        },
        {
          "type": "null"
        }
      ],
      "default": null,
      "description": "Excel shape type name.",
      "title": "Type"
    },
    "w": {
      "anyOf": [
        {
          "type": "integer"
        },
        {
          "type": "null"
        }
      ],
      "default": null,
      "description": "Shape width (None if unknown).",
      "title": "W"
    }
  },
  "required": [
    "text",
    "l",
    "t"
  ],
  "title": "Shape",
  "type": "object"
}